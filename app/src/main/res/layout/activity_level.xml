<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_evolution"
    tools:context=".LevelActivity">


    <ImageView
        android:id="@+id/textView_difficile3"
        android:layout_width="221dp"
        android:layout_height="64dp"
        android:layout_marginTop="68dp"
        android:background="@drawable/evolution_titre"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/tableLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.373" />

    <ImageView
        android:id="@+id/back_to_evolution"
        android:layout_width="@dimen/wight_backbutton"
        android:layout_height="@dimen/height_backbutton"
        android:layout_marginTop="44dp"
        android:src="@drawable/ic_baseline_arrow_back_ios_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TableLayout
        android:id="@+id/tableLayout3"
        android:layout_width="@dimen/wight_imageview"
        android:layout_height="301dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="32dp">

            <ImageView
                android:id="@+id/multifactor_evolution"
                android:layout_width="@dimen/wight_imageview"
                android:layout_height="@dimen/height_imageview"
                app:srcCompat="@drawable/multifactor"
                tools:layout_editor_absoluteX="95dp"
                tools:layout_editor_absoluteY="268dp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="32dp">

            <ImageView
                android:id="@+id/mathemaquizz_evolution"
                android:layout_width="@dimen/wight_imageview"
                android:layout_height="@dimen/height_imageview"
                app:srcCompat="@drawable/mathemaquizz"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="371dp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/roulette_evolution"
                android:layout_width="@dimen/wight_imageview"
                android:layout_height="@dimen/height_imageview"
                app:srcCompat="@drawable/roulette_boutton"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="568dp" />
        </TableRow>
    </TableLayout>

    <TableLayout
        android:layout_width="32dp"
        android:layout_height="302dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/tableLayout3"
        app:layout_constraintTop_toTopOf="parent">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="79dp">

            <ImageView
                android:id="@+id/info_multifactor_evolution"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@android:drawable/ic_menu_info_details"
                tools:layout_editor_absoluteX="343dp"
                tools:layout_editor_absoluteY="655dp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="79dp">

            <ImageView
                android:id="@+id/info_mathemaquizz_evolution"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@android:drawable/ic_menu_info_details"
                tools:layout_editor_absoluteX="311dp"
                tools:layout_editor_absoluteY="647dp" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/info_roulette_evolution"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@android:drawable/ic_menu_info_details"
                tools:layout_editor_absoluteX="319dp"
                tools:layout_editor_absoluteY="631dp" />

        </TableRow>
    </TableLayout>


</androidx.constraintlayout.widget.ConstraintLayout>